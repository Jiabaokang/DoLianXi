[
  {
    "description": "这是一个带headers的请求头信息的请求post",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "json": {
        "name": "wangwu",
        "age": "18"
      }
    },
    "response": {
      "json": {
        "wangwu": "success",
        "status": "1"
      }
    }
  }
]
